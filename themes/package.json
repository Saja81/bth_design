{
  "name": "themes",
  "version": "1.0.0",
  "main": "index.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "stylelint \"**/*.scss\"",
    "watch": "sass atzjodark/scss/style.scss:atzjodark/css/style.css atzjodark/scss/style-dark.scss:atzjodark/css/style-dark.css --no-source-map --watch",
    "watch-min": "sass atzjodark/scss/style.scss:atzjodark/css/style.min.css atzjodark/scss/style-dark.scss:atzjodark/css/style-dark.min.css --no-source-map --watch --style compressed",
    "style": "npm run style-light && npm run style-dark",
    "style-min": "npm run style-light-min && npm run style-dark-min",
    "style-dark": "sass atzjodark/scss/style-dark.scss atzjodark/css/style-dark.css --no-source-map",
    "style-dark-min": "sass atzjodark/scss/style-dark.scss atzjodark/css/style-dark.min.css --no-source-map --style compressed",
    "style-light": "sass atzjodark/scss/style.scss atzjodark/css/style.css --no-source-map",
    "style-light-min": "sass atzjodark/scss/style.scss atzjodark/css/style.min.css --no-source-map --style compressed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "normalize.css": "^8.0.1",
    "sass": "^1.82.0",
    "stylelint": "^16.11.0",
    "stylelint-config-sass-guidelines": "^12.1.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.6.0"
  },
  "description": ""
}
